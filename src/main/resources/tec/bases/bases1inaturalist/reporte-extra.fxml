<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tec.bases.bases1inaturalist.ShowReporteExtra">
    <Label style="-fx-font-size: 18; -fx-font-weight: bold;" text="Reporte Extra" />

    <TableView fx:id="tableView" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="idUserCol" prefWidth="83.0" text="ID Usuario" />
            <TableColumn fx:id="idPersonCol" prefWidth="99.0" text="ID Persona" />
            <TableColumn fx:id="idIdentifCol" prefWidth="168.0" text="ID Identificación" />
            <TableColumn fx:id="nombreColumn" prefWidth="320.0" text="Nombre" />
            <TableColumn fx:id="countryColumn" prefWidth="179.0" text="País" />
        </columns>
    </TableView>

    <Separator prefHeight="10.0" prefWidth="600.0" />

    <HBox alignment="CENTER" spacing="100.0">
        <Button fx:id="returnButton" onAction="#launchMainMenu" text="Volver al Menú" textAlignment="CENTER" />
        <padding>
            <Insets bottom="5.0" />
        </padding>
    </HBox>
</VBox>
